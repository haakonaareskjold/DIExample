{
    "name": "haakon/di",
    "require": {
        "php": "7.4.*"
    },
    "autoload": {
        "psr-4": {
            "QA\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QA\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "^3.5.5"
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "phpcs": "bin/phpcs --standard=PSR12 ./src",
        "phpcbf": "bin/phpcbf --standard=PSR12 ./src",
        "test": "bin/phpunit ./tests"
    }
}
